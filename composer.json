{
    "name": "julibo/msfoole",
    "description": "基于swoole的简易微服务框架",
    "type": "library",
    "keywords": [
        "msfoole",
        "swoole",
        "php微服务框架"
    ],
    "homepage": "http://msfoole.julibo.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "carson",
            "email": "yuzhanwei@aliyun.com"
        }
    ],
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "ext-swoole": ">=2.1.3",
        "monolog/monolog": "~1.9",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/console": "~4.0"
    },
    "require-dev" : {

    },
    "autoload": {
        "psr-4": {
            "Msfoole\\": "src\\"

        }
    },
    "extra": {
    },
    "config": {
        "process-timeout": 1800,
        "vendor-dir": "vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
