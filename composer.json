{
    "name": "julibo/msfoole",
    "description": "基于swoole的简易微服务框架",
    "type": "library",
    "keywords": [
        "msfoole",
        "swoole",
        "php微服务框架"
    ],
    "homepage": "http://msfoole.julibo.com/",
    "license": "Apache-2.0",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "authors": [
        {
            "name": "carson",
            "email": "yuzhanwei@aliyun.com",
            "homepage": "http://msfoole.julibo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-swoole": ">=4.1.0",
        "guzzlehttp/guzzle": "~6.3.0",
        "symfony/console": "~4.1.6",
        "monolog/monolog": "~1.23.0",
        "topthink/think-orm": "~1.2.13",
        "topthink/think-log": "~1.0",
        "topthink/think-cache": "~1.0",
        "topthink/think-container": "~1.0",
        "topthink/think-helper": "~1.0.6",
        "topthink/think-validate": "～1.0.1"
        
    },
    "require-dev" : {},
    "autoload": {
        "psr-4": {
            "Julibo\\Msfoole\\": "src\\"
        },
        "files": [
            "src/base.php",
            "src/config.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {},
    "config": {
        "vendor-dir": "vendor",
        "bin-dir": "vendor/bin",
        "preferred-install": "dist"
    }
}
